---
interface Props {
  src: any;
  alt: string;
  width?: number | string;
  height?: number | string;
  class?: string;
  loading?: "eager" | "lazy";
  decoding?: "async" | "auto" | "sync";
}

const {
  src,
  alt,
  width,
  height,
  class: className,
  loading,
  decoding,
} = Astro.props;
---

<img
  src={src.src || src}
  alt={alt}
  width={width}
  height={height}
  class={className}
  loading={loading}
  decoding={decoding}
/>

